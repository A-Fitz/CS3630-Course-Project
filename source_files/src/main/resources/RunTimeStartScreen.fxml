<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane minHeight="450.0" minWidth="800.0" prefHeight="720.0" prefWidth="1280.0"
          xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="ui.controllers.RunTimeStartScreenController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="22.0" percentHeight="5.0" prefHeight="36.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="428.0" percentHeight="90.0" prefHeight="684.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="22.0" percentHeight="5.0" prefHeight="36.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label fx:id="welcomeLabel" GridPane.columnIndex="0" GridPane.rowIndex="0" text="Welcome">
            <font>
                <Font name="System Bold" size="20.0" />
            </font>
        </Label>
        <GridPane fx:id="startGridPane" GridPane.columnIndex="0" GridPane.rowIndex="1" alignment="CENTER"
                  hgap="50.0" vgap="50.0">
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" percentWidth="25.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" percentWidth="25.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" percentWidth="25.0"/>
                <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" percentWidth="25.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints percentHeight="20.0" valignment="CENTER" vgrow="SOMETIMES"/>
                <RowConstraints percentHeight="20.0" valignment="CENTER" vgrow="SOMETIMES"/>
                <RowConstraints percentHeight="20.0" valignment="CENTER" vgrow="SOMETIMES"/>
                <RowConstraints percentHeight="20.0" valignment="CENTER" vgrow="SOMETIMES"/>
                <RowConstraints percentHeight="20.0" valignment="CENTER" vgrow="SOMETIMES"/>
            </rowConstraints>
            <padding>
                <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
            </padding>
            <GridPane.margin>
                <Insets bottom="5.0"/>
            </GridPane.margin>
            <children>

            </children>
        </GridPane>
        <Label fx:id="messageLabel" GridPane.columnIndex="0" GridPane.rowIndex="2" alignment="CENTER"
               GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</GridPane>

